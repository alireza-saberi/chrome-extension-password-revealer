{
	"name":"Password Revealer",
	"version":"0.1",
	"manifest_version":2,
	"description":"It reveals the password type inputs on the web pages",
	"icons":{
			"64":"decrypted.png"
		},
	"browser_action":{
			"default_icon":"decrypted.png",
			"default_title":"Password Revealer!",
			"popup":"popup.html"
		},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["myscript.js"],
	  "run_at":"document_end",
	  "all_frames":true
    }
  ]
}